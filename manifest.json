{
  "manifest_version": 2,

  "name": "Live stream list",
  "description": "Showing you a list of your favorite Twitch streamers currently streaming.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Check who's live!"
  },
  "content_security_policy":
    "script-src 'self' https://wind-bow.glitch.me https://wind-bow.hyperdev.space; object-src 'self'",
  "content_scripts": [ {
    "js": [ "jquery.min.js", "assets/js/index.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}